    "l = eval(input(\"Enter a list of numbers \"))\n",
    "output = []\n",
    "for i in l:\n",
    "    if i%2!=0:\n",
    "        output.append(i)\n",
    "        # output += [i]\n",
    "print(output)"
   ]
